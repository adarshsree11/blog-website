<%- include('partials/header') %>

<h1>Compose</h1>

<form class="form-group" action="/compose" method="post">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" autofocus autocomplete="off"></input>
    <label>Post</label>
    <textarea class="form-control" name="postBody" cols="40" rows="5"></textarea>
    <button id="publishBtn" class="btn-primary btn" type="submit" name="button">Publish</button>
</form>



<%- include('partials/footer') %> 